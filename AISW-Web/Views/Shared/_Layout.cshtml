<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>AI Services Workshop</title>

    <!--Fav icons - generate your own at http://realfavicongenerator.net -->
    <link rel="apple-touch-icon" sizes="180x180" href="/images/favicon/apple-touch-icon.png">
    <link rel="icon" type="image/png" href="/images/favicon/favicon-32x32.png" sizes="32x32">
    <link rel="icon" type="image/png" href="/images/favicon/favicon-16x16.png" sizes="16x16">
    <link rel="manifest" href="/images/favicon/manifest.json">
    <link rel="mask-icon" href="/images/favicon/safari-pinned-tab.svg" color="#000000">
    <link rel="shortcut icon" href="/images/favicon/favicon.ico">
    <meta name="msapplication-config" content="/images/favicon/browserconfig.xml">
    <meta name="theme-color" content="#ffffff">

    <!--CSS-->
    <environment names="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
        <link rel="stylesheet" href="~/css/EurostileLTStd-BoldEx2.css" />
        <link rel="stylesheet" href="~/css/EurostileLTStd-BoldCn.css" />
        <link rel="stylesheet" href="~/lib/font-awesome/css/font-awesome.min.css" />
        <link rel="stylesheet" href="~/css/site.css" />
    </environment>
    <environment names="Staging,Production">
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    </environment>

    @RenderSection("head", required: false)
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a asp-controller="Home" asp-action="Index" class="navbar-brand">AI Day</a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a href="#">Where & When</a></li>
                    <li><a href="#">Agenda</a></li>
                    <li><a href="#">Before you arrive</a></li>
                    <li><a href="#">Links</a></li>
                    <li><a href="#">Survey</a></li>
                </ul>
                <ul class="nav navbar-nav navbar-right hidden-xs">
                    <li><a href="#"><i class="fa fa-slack fa-lg" aria-hidden="true"></i></a></li>
                    <li><a href="#"><i class="fa fa-twitter fa-lg" aria-hidden="true"></i></a></li>
                    <li><a href="#"><i class="fa fa-github fa-lg" aria-hidden="true"></i></a></li>
                    <li><a href="@string.Format("mailto:martin.kearn@microsoft.com?Subject=Enquiry%20from%20AISW")"><i class="fa fa-envelope-o fa-lg footer-icon" aria-hidden="true"></i></a></li>
                </ul>
            </div>
        </div>
    </div>

    @RenderBody()

    <footer class="footer container-fluid">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <p>Don't be a stranger...</p>
                    <a href="#"><i class="fa fa-slack fa-lg footer-icon" aria-hidden="true"></i></a>
                    <a href="#"><i class="fa fa-twitter fa-lg footer-icon" aria-hidden="true"></i></a>
                    <a href="#"><i class="fa fa-github fa-lg footer-icon" aria-hidden="true"></i></a>
                    <a href="@string.Format("mailto:martin.kearn@microsoft.com?Subject=Enquiry%20from%20AISW")"><i class="fa fa-envelope-o fa-lg footer-icon" aria-hidden="true"></i></a>
                </div>
            </div>
        </div>
    </footer>

    <!--JS-->
    <script src="~/js/site.min.js" defer></script>

    @RenderSection("scripts", required: false)
</body>
</html>
